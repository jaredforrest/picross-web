{
  "jsc": {
    "parser": {
      "syntax": "ecmascript"
    },
    "target": "es2017",
    "loose": true,
    "minify": {
      "compress": {
        "unused": true
      },
      "mangle": true
      }
  },
  "module": {
    "type": "es6",
    "ignoreDynamic": true
  },
  "minify": true,
  "isModule": true
}
